<freecast>
 	<node>
  		<peerprovider>
			<!-- specifies where the tracker lives -->
    		<trackeraddress>
    			<host>localhost</host>
    		</trackeraddress>
  		</peerprovider>
		<receiver>
			<!-- the nodes connects to the specified source stream server -->
			<class>shoutclient</class>
			<!-- Just a test stream, visit http://www.fluendo.com -->
   			<url>http://stream.fluendo.com:8800</url>
  		</receiver>
	</node>
</freecast>
