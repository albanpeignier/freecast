<!-- 
	 FreeCast configuration file
	 visit http://www.freecast.org/userguide/node-configuration.html
-->
<freecast>
 	<node>
  		<peerprovider>
			<!-- specifies where the tracker lives -->
    		<trackeraddress>
    			<host>localhost</host>
    		</trackeraddress>
  		</peerprovider>
  		<players>
   			<player>
   				<!-- play the stream in the sound card -->
   				<class>audio</class>
   			</player>
  		</players>
	</node>
</freecast>
